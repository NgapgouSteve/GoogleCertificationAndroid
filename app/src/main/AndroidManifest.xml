<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tincs.googlecertification">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".androidcore.NotifSheduler"></activity>
        <service
            android:name=".androidcore.NotificationJobService"
            android:permission="android.permission.BIND_JOB_SERVICE"/>

        <activity android:name=".androidcore.NotifActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".androidcore.AndroidCore" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>